{
  "name": "pino-prettifier",
  "version": "1.0.6",
  "description": "Development mode prettifier for Pino",
  "keywords": [
    "lifion",
    "logger",
    "logging",
    "pino",
    "prettifier"
  ],
  "author": "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
  "maintainers": [
    "Chen Doron <Chen.Doron@ADP.com>",
    "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
    "Jenny Eckstein <Jenny.Eckstein@ADP.com>",
    "Zaid Masud <Zaid.Masud@ADP.com>"
  ],
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifion/pino-prettifier.git"
  },
  "bugs": {
    "url": "https://github.com/lifion/pino-prettifier/issues"
  },
  "homepage": "https://github.com/lifion/pino-prettifier#readme",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.json --ignore-pattern='!.*.*'",
    "format": "prettier --write '**/*.{md,js,json}' '!coverage/**/*.{js,json}'",
    "jest": "jest -c ./.jest.json",
    "prepare": "check-engines",
    "test": "npm run jest -- --ci --runInBand && codecov",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {},
  "devDependencies": {
    "auto-changelog": "^1.16.0",
    "check-engines": "^1.5.0",
    "codecov": "^3.5.0",
    "eslint": "^6.3.0",
    "eslint-config-lifion": "^1.3.0",
    "hex-debug": "^2.0.2",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint && npm run jest"
    }
  },
  "@lifion/core-commons": {
    "autoDocs": false,
    "template": "public",
    "updated": "2019-08-27T00:08:18.771Z",
    "version": "2.3.3"
  }
}
